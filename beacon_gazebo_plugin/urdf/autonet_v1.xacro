<?xml version="1.0" ?>
<robot name="autonet_v1" 
  xmlns:xacro="http://www.ros.org/wiki/xacro">

<!--  <xacro:include filename="$(find beacon_gazebo_plugin)/urdf/materials.xacro" />-->
  <xacro:include filename="$(find beacon_gazebo_plugin)/urdf/autonet_v1_gazebo.xacro" />
  <xacro:include filename="$(find beacon_gazebo_plugin)/urdf/macros.xacro" />

  <link name="base_link">
    <!-- pose and inertial -->
    <pose>-0.1 0 0.05 0 0 0</pose>
    <inertial>
      <mass value="0.5"/>
      <origin rpy="0 0 0" xyz="-0.1 0 0.05"/>
      <inertia ixx="0.206208333333" ixy="0" ixz="0" iyy="0.206208333333" iyz="0" izz="0.206208333333"/>
    </inertial>
    <!-- body -->
    <collision name="collision_chassis">
      <geometry>
        <box size="0.295 0.295 0.05"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 0 0" xyz="-0.1 0 0"/>
      <geometry>
        <box size="0.295 0.295 0.05"/>
      </geometry>
<!--      <material name="blue"/>-->
    </visual>
    <!-- caster front -->
    <collision name="caster_front_collision">]
      <origin rpy="0 1.5707 0" xyz="0.05 0 -0.012"/>
      <geometry>
        <cylinder length="0.025" radius="0.038"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>
    <!-- <visual name="caster_front_visual">
      <origin rpy="0 1.5707 0" xyz="0.05 0 -0.012"/>
      <geometry>
        <cylinder length="0.025" radius="0.038"/>
      </geometry>
    </visual> -->
    <collision name="caster_back_collision">]
      <origin rpy="0 1.5707 0" xyz="-0.25 0 -0.012"/>
      <geometry>
        <cylinder length="0.025" radius="0.038"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>
    <visual name="caster_back_visual">
      <origin rpy="0 1.5707 0" xyz="-0.25 0 -0.012"/>
      <geometry>
        <cylinder length="0.025" radius="0.038"/>
      </geometry>
    </visual>
  </link>

  <!-- <link name="sensor_laser">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="1" />
      <xacro:cylinder_inertia mass="1" r="0.05" l="0.1" />

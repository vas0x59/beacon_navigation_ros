  <!-- <joint name="joint_sensor_laser" type="fixed">
    <origin xyz="0.15 0 0.05" rpy="0 0 0"/>
    <parent link="link_chassis"/>
    <child link="sensor_laser"/>
  </joint>
   -->
  <xacro:link_wheel name="link_right_wheel" />
  <xacro:joint_wheel name="joint_right_wheel" child="link_right_wheel" origin_xyz="0 0.16 -0.012" />

  <xacro:link_wheel name="link_left_wheel" />
  <xacro:joint_wheel name="joint_left_wheel" child="link_left_wheel" origin_xyz="0 -0.16 -0.012" />

  <joint name="front_bumper_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin xyz="0.08 0 0" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="front_bumper_link"/>
  </joint>
  <gazebo reference='front_bumper_joint'>
    <preserveFixedJoint>true</preserveFixedJoint>
  </gazebo>
  <joint name="imu_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin xyz="0 0 0.01" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="imu_link"/>
  </joint>
  <gazebo reference='imu_joint'>
    <preserveFixedJoint>true</preserveFixedJoint>
  </gazebo>
  <joint name="receiver_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin xyz="0 0 0.05" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="receiver_link"/>
  </joint>
  <gazebo reference='receiver_joint'>
    <preserveFixedJoint>true</preserveFixedJoint>
  </gazebo>
<!--  &lt;!&ndash; Camera &ndash;&gt;-->
  <link name="front_bumper_link">
    <pose>0 0 0 0 0 0</pose>
    <collision name="front_bumper_col">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.04 0.20 0.04"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.04 0.20 0.04"/>
      </geometry>
<!--      <material name="red"/>-->
    </visual>

    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
  </link>

  <link name="imu_link">
    <pose>0 0 0 0 0 0</pose>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.04 0.04 0.02"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.04 0.04 0.04"/>
      </geometry>
<!--      <material name="green"/>-->
    </visual>

    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
  </link>

  <link name="receiver_link">
    <pose>0 0 0 0 0 0</pose>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.02 0.02 0.04"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.02 0.02 0.04"/>
      </geometry>
      <!--      <material name="green"/>-->
    </visual>
    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
  </link>

